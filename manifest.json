{
  "manifest_version": 2,

  "name": "Reading List",
  "description": "This extension is for managing a reading list",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "reading_list.html",
    "default_title": "Reading List"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}